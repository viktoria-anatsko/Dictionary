         <children>
            <TextField layoutX="18.0" layoutY="21.0" prefHeight="31.0" prefWidth="264.0" />
            <ListView layoutX="18.0" layoutY="71.0" prefHeight="290.0" prefWidth="264.0" fx:id="listView"/>
